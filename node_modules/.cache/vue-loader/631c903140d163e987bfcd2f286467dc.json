tem\"\n      placeholder=\"输入待完成任务，按回车键添加\"\n      @keyup.enter=\"add\"\n    />\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { defineComponent, ref, reactive } from \"vue\";\nimport { Todo } from \"@/types/todo\";\nexport default defineComponent({\n  name: \"Head\",\n  // props: {\n  //   addToPlan: {\n  //     type: Function,\n  //     required: true,\n  //   },\n  // },\n  setup(props, { emit }) {\n    const todoItem = ref(\"\");\n    const add = () => {\n      const text = todoItem.value;\n      if (!text.trim()) return;\n      const todo: Todo = {\n        id: Date.now(),\n        title: text.trim(),\n        isCompleted: false,\n      };\n      // // setup中不能直接调用props中的内容\n      // // 因此此处用到setup的props参数\n      emit(\"addToPlan\", todo);\n      // props.addToPlan(todo);\n      todoItem.value = \"\";\n    };\n    return {\n      todoItem,\n      add,\n    };\n  },\n});\n</script>\n\n<style scoped>\n.header-wrap input {\n  width: 100%;\n  padding: 5px;\n  border-radius: 5px;\n  outline: none;\n  border: 1px solid lightblue;\n  box-shadow: 1px 1px 1px rgba(153, 90, 90, 0.2);\n}\n</style>\n"]}]}