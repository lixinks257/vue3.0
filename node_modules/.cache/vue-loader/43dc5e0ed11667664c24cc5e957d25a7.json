CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EAC7B,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/lixin/iCloud Drive（归档）/Documents/code/vue3学习/vue3-project/src/App2.vue","sourceRoot":"","sourcesContent":["<template>\n  <h1>reactive的使用</h1>\n  <h3>{{ user.name }}</h3>\n  <h3>{{ user.age }}</h3>\n  <h3>性别: {{ user.gender }}</h3>\n  <button @click=\"updateUser\">更新user</button>\n</template>\n\n<script lang=\"ts\">\nimport { defineComponent, reactive } from \"vue\";\n\n// 暴露出去一个定义好的组件\nexport default defineComponent({\n  name: \"App\",\n  // 函数的入口\n  setup() {\n    const obj = {\n      name: \"SS\",\n      age: 1,\n    };\n    // 对象的响应式 1.接收一个普通对象，然后返回该对象的响应式代理对象\n    const user = reactive(obj);\n    // const updateUser = () => {\n    //   user.name = \"XXX\";\n    //   user.age = 10;\n    //   user.gender = \"女\";\n    //   delete user.name;\n    // };\n    function updateUser() {\n      user.name = \"XXX\";\n      user.age = 10;\n      // 通过代理对象新增和删除值，被代理的的对象属性值也会跟着改变\n      user.gender = '女';\n      // delete user.name\n    }\n    console.log(\"user\", user);\n    return {\n      user,\n      updateUser,\n    };\n  },\n});\n</script>\n"]}]}